{
  "enabledManagers": ["gitlabci", "docker-compose", "pre-commit"],
  "labels": ["dependencies", "docker"],
  "docker-compose": {
    "commitMessagePrefix": "dep:",
    "commitBody": "changelog: dependency"
  },
  "gitlabci": {
    "commitMessagePrefix": "dep-dev:",
    "commitBody": "changelog: ci"
  },
  "pre-commit": {
    "enabled": true,
    "commitBody": "changelog: chore"
  }
}
