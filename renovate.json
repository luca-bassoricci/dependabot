  "enabledManagers": [
    "gitlabci",
    "docker-compose",
    "pre-commit"
  ],
  "labels": [
    "dependencies"
  ],
  "docker-compose": {
    "commitMessagePrefix": "dep:",
    "commitBody": "changelog: dependency",
    "addLabels": [
      "docker"
    ]
  },
  "gitlabci": {
    "commitBody": "changelog: ci",
    "addLabels": [
      "docker"
    ]
  },
  "pre-commit": {
    "commitBody": "changelog: chore"
  }
}
